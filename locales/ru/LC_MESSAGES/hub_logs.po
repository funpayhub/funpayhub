# ----------------------------------------------------
# -------------------- Plugins logs ------------------
# ----------------------------------------------------
msgid "Adding %s to %s."
msgstr "Добавляю %s в %s."

msgid "Ignoring %s: not a directory."
msgstr "Игнорирую %s: не директория."

msgid "Loading plugin %s."
msgstr "Загружаю плагин %s."

msgid "Loading manifest for %s."
msgstr "Загружаю манифест плагина %s."

msgid "Unable to load plugin manifest for %s. Skipping."
msgstr "Не удалось загрузить манифест плагина %s. Пропускаю."

msgid "Plugin %s is already loaded. Skipping."
msgstr "Плагин %s уже загружен. Пропускаю."

msgid "FunPay Hub version mismatch. Expected: %s. Current: %s."
msgstr "Несоответствие версии FunPayHub. Ожидалось: %s. Текущая: %s."

msgid "Plugin %s is disabled. Skipping."
msgstr "Плагин %s отключен. Пропускаю."

msgid "Loading entry point %s of %s."
msgstr "Загружаю точку входа %s плагина %s."

msgid "Plugin %s does not have a locales path. Skipping locales installation."
msgstr "У плагина %s не указан путь до локалей. Пропускаю установку локалей."

msgid "Loading locales for plugin %s."
msgstr "Загружаю локали плагина %s."

msgid "Unable to load locales for plugin %s. This will not stop plugin from setting up."
msgstr "Не удалось загрузить локали плагина %s. Это не остановит загрузку плагина."

msgid "Plugin %s does not have a plugin instance. Skipping step %s."
msgstr "Плагин %s не инстанцирован. Пропускаю этап %s."

msgid "Running %s step for plugin %s."
msgstr "Выполняю этап %s для плагина %s."

msgid "Installing plugin from source %s ..."
msgstr "Устанавливаю плагин из источника %s ..."

msgid "Failed to install plugin from source %s."
msgstr "Не удалось устоановить плагин из источника %s."


# ----------------------------------------------------
# -------------------- Offer raiser ------------------
# ----------------------------------------------------
msgid "An 429 error occurred while raising offers of category %s. Waiting for %d seconds."
msgstr "Произошла ошибка 429 при поднятии лотов категории %s. Ожидаю %d секунд."

msgid "Offer raiser loop for category %s has been started."
msgstr "Цикл поднятия лотов категории %s запущен."

msgid "Offers of category %s has been raised. Next try in %d."
msgstr "Лоты категории %s подняты. Следующая попытка через %d."

msgid "Unable to raise offers of category %s: not authorized."
msgstr "Не удалось поднять лоты категории %s: не авторизирован."

msgid "Unable to raise offers of category %s: need to wait for %d."
msgstr "Не удалось поднять лоты категории %s: необходимо подождать %d."

msgid "A Server error occurred while raising offers of category %s."
msgstr "Произошла серверная ошибка при поднятии лотов категории %s."

msgid "An unexpected error occurred while raising offers of category %s."
msgstr "Произошла неожиданная ошибка при поднятии лотов категории %s."

msgid "Offer raising loop of category %s has been stopped."
msgstr "Цикл поднятия лотов категории %s остановлен."

msgid "Offer raising loop task of category %s (%s) has been removed from manager."
msgstr "Таска цикла поднятия лотов категории %s (%s) удалена из менеджера."

msgid "Offer raising loop task of category %s (%s) has not been found in manager."
msgstr "Таска цикла поднятия лотов категории %s (%s) не найдена."


# ----------------------------------------------------
# ---------------------- Updater ---------------------
# ----------------------------------------------------
msgid "Fetching releases..."
msgstr "Запрашиваю данные о релизах..."

msgid "An error occurred while fetching releases."
msgstr "Произошла ошибка при запросе данных о релизах."

msgid "No releases available."
msgstr "Установлена последняя версия FunPay Hub."

msgid "New release %s available."
msgstr "Доступен новый релиз %s."

msgid "Downloading update from %s"
msgstr "Загружаю обновление из %s"

msgid "Unexpected status code while downloading update: %d."
msgstr "Получен неожиданный статус код при загрузке обновления: %d."

msgid "Timeout error while downloading updated."
msgstr "Таймаут скачивания обновления."

msgid "Unexpected error while downloading an updated."
msgstr "Произошла неожиданная ошибка при скачивании обновления."

msgid "Installing update to %s ..."
msgstr "Устанавливаю обновление %s ..."

msgid "Unexpected error while installing update to %s."
msgstr "Произошла неожиданная ошибка при установке обновления %s."


